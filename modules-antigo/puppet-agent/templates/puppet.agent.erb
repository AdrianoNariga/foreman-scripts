[main]
vardir = /var/lib/puppet
logdir = /var/log/puppet
rundir = /var/run/puppet
ssldir = $vardir/ssl

[agent]
pluginsync      = true
report          = true
ignoreschedules = true
daemon          = false
environment     = home_jab
ca_server       = <%= puppet_ca_server %>
server          = <%= puppetmaster %>
certname        = <%= fqdn %>
