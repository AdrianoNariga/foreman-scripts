source /etc/network/interfaces.d/*

auto lo
iface lo inet loopback

allow-hotplug <%= bridge %>

auto br0
iface br0 inet static
   address <%= ip %>
   netmask <%= mask %>
   gateway <%= gw %>
   dns-nameservers <%= dns %>
   dns-search <%= domain %>
   bridge_ports <%= bridge %>
   bridge_stp off
   bridge_fd 0
